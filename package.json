{
  "name": "book-a-meal",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:iverenshaguy/book-a-meal-server.git",
  "author": "Iveren Shaguy <iverenshaguy@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "build": "run-s build:v1 build:v2",
    "build:v1": "babel --root-mode upward ./src -d ./dist",
    "build:v2": "nest build",
    "coverage": "run-s sed:lcov merge:lcov push:coverage:coveralls",
    "create:coverage:folder": "rm -rf ./coverage && mkdir ./coverage",
    "db:migrate": "npm run sequelize-cli:es6 db:migrate",
    "db:migrate:undo": "npm run sequelize-cli:es6 db:migrate:undo:all",
    "db:seed": "npm run sequelize-cli:es6 db:seed:all",
    "db:setup": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
    "db:setup:test": "NODE_ENV=test npm run db:setup",
    "format:v2": "prettier --write \"v2/src/**/*.ts\" \"v2/tests/**/*.ts\"",
    "lint": "run-s lint:v1",
    "lint:v1": "eslint '**/*.{js,jsx}' --fix",
    "lint:v2": "eslint --config ./v2/.eslintrc.js  \"/{v2/src,apps,libs,test}/**/*.ts\" --fix",
    "merge:lcov": "cat ./coverage/lcov-v1.info ./coverage/lcov-v2.info > ./coverage/lcov.info",
    "prebuild:v2": "rimraf ./v2/dist",
    "pretest": "yarn lint",
    "push:coverage:coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "node ./dist/app.js",
    "start:dev": "nodemon --exec babel-node --root-mode upward ./src/app.js",
    "start:debug:v2": "nest start --debug --watch",
    "start:dev:v2": "nest start --watch",
    "start:e2e": "NODE_ENV=e2e babel-node --root-mode upward ./src/app.js",
    "start:prod:v2": "node v2/dist/main",
    "start:v2": "nest start",
    "sed:lcov": "run-s sed:lcov:v1 sed:lcov:v2",
    "sed:lcov:v1": "sed 's/SF:/SF:\\//g' ./coverage/lcov.info > ./coverage/lcov-v1.info",
    "sed:lcov:v2": "sed 's/SF:/SF:\\//g' ./v2/coverage/lcov.info > ./coverage/lcov-v2.info",
    "sequelize-cli:es6": "babel-node ./node_modules/.bin/sequelize $*",
    "test": "run-s test:v1 test:v2",
    "test:coverage": "run-s test:cov:v1 test:cov:v2 sed:lcov",
    "test:v1": "yarn db:setup:test && NODE_ENV=test mocha --require tests/config ./tests/test.js --timeout 50000 --exit",
    "test:v2": "yarn db:setup:test && yarn jest --detectOpenHandles",
    "test:cov:v1": "nyc --reporter=lcov --reporter=json --reporter=text npm run test:v1",
    "test:cov:v2": "yarn db:setup:test && yarn jest --detectOpenHandles --coverage",
    "test:debug:v2": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:v2": "jest --config ./jest-e2e.json --detectOpenHandles --coverage",
    "test:watch:v1": "npm run test:v1 --watch",
    "test:watch:v2": "yarn db:setup:test && yarn jest --detectOpenHandles --watch"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/sequelize": "^0.1.0",
    "@nestjs/serve-static": "^2.1.3",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.3",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "csurf": "^1.11.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^5.1.2",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^8.2.1",
    "mockdate": "^2.0.2",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "npm-run-all": "^4.1.2",
    "parse-database-url": "^0.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "7.4.1",
    "pg-hstore": "^2.3.2",
    "random-string": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sequelize": "^6.3.4",
    "sequelize-typescript": "^2.0.0-beta.0",
    "service": "^0.1.4",
    "uuid": "^3.2.1",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.4.4",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^84.0.1",
    "coveralls": "^3.0.5",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^3.0.2",
    "istanbul": "^0.4.5",
    "jest": "26.0.1",
    "mocha": "^6.2.0",
    "nightwatch": "^1.1.13",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "selenium-server": "^3.141.59",
    "selenium-standalone": "^6.14.1",
    "sequelize-cli": "^5.5.0",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "sync-dotenv": "^2.2.0",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  }
}
